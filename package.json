{
  "name": "omnic",
  "version": "0.0.0",
  "description": "Epic declarative API client",
  "main": "lib/cjs.js",
  "module": "lib/esnext.js",
  "esnext": "lib/esnext.js",
  "unpkg": "lib/umd.js",
  "types": "types",
  "scripts": {
    "test": "jest",
    "build": "node build",
    "preversion": "npm run docs && npm run build && (git add .) && git commit -m Docs-and-build.",
    "patch": "npm version patch && npm publish && git push",
    "minor": "npm version minor && npm publish && git push",
    "major": "npm version major && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KazanExpress/omnic.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KazanExpress/omnic/issues"
  },
  "homepage": "https://github.com/KazanExpress/omnic#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-transform-async-to-promises": "^0.5.5",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-jest": "^22.4.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "es6-promise": "^4.2.4",
    "fs-extra": "^5.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-regenerator": "^0.5.4",
    "rollup-plugin-uglify": "^3.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/tests/**/?(*.)test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
