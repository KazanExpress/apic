{
  "compilerOptions": {
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise",
      "es2016",
      "es2017",
      "esnext"
    ],
    "target": "es2015",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false
  }
}
